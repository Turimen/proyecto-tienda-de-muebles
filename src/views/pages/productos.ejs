<!DOCTYPE html>
<html lang="en">
  <head>
    <%-include("../partials/head.ejs")%>
  </head>
  <body>
    <nav><%-include("../partials/nav.ejs")%></nav>
    <main class="cont-padre">
      <%data.forEach((prod)=>{%>
      <div class="card card-prod">
        <h2 class="titulo-prod"><%= prod.producto%></h2>
        <img
          class="img-card img-prod"
          src="<%=prod.imagen%>"
          alt="imagen=producto"
        />
        <p class="desc-card"><%=prod.descripcion%></p>
        <p><%=prod.precio%></p>
        <a class="boton-ver" href="#">Comprar</a>
      </div>
      <%})%>
    </main>
  </body>
</html>
